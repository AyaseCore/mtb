        <CheckBox x:Name="itemFlag6" Content="RIGHTCLICK ACTIVATABLE" HorizontalAlignment="Left" Margin="10,148,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="itemFlag7" Content="NO EQUIP COOLDOWN" HorizontalAlignment="Left" Margin="10,171,0,0" VerticalAlignment="Top"/>
