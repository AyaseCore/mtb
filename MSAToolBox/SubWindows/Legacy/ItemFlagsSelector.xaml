        <CheckBox x:Name="itemFlag13" Content="CHARTER" HorizontalAlignment="Left" Margin="10,125,0,0" VerticalAlignment="Top" Grid.Column="1"/>
        <CheckBox x:Name="itemFlag14" Content="UNK5" HorizontalAlignment="Left" Margin="10,148,0,0" VerticalAlignment="Top" Grid.Column="1"/>
        <CheckBox x:Name="itemFlag15" Content="UNK6" HorizontalAlignment="Left" Margin="10,171,0,0" VerticalAlignment="Top" Grid.Column="1"/>
        <CheckBox x:Name="itemFlag16" Content="UNK7" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Grid.Column="2"/>
        <CheckBox x:Name="itemFlag17" Content="UNK8" HorizontalAlignment="Left" Margin="10,33,0,0" VerticalAlignment="Top" Grid.Column="2"/>
        <CheckBox x:Name="itemFlag18" Content="PROSPECTABLE" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" Grid.Column="2"/>
        <CheckBox x:Name="itemFlag19" Content="UNIQUE EQUIPPED" HorizontalAlignment="Left" Margin="10,79,0,0" VerticalAlignment="Top" Grid.Column="2"/>
        <CheckBox x:Name="itemFlag20" Content="UNK9" HorizontalAlignment="Left" Margin="10,102,0,0" VerticalAlignment="Top" Grid.Column="2"/>
        <CheckBox x:Name="itemFlag21" Content="USEABLE IN ARENA" HorizontalAlignment="Left" Margin="10,125,0,0" VerticalAlignment="Top" Grid.Column="2"/>
        <CheckBox x:Name="itemFlag22" Content="THROWABLE" HorizontalAlignment="Left" Margin="10,148,0,0" VerticalAlignment="Top" Grid.Column="2"/>
