<Controls:MetroWindow x:Class="MSAToolBox.SubWindows.Legacy.SpellCastInterruptSelector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Controls="http://metro.mahapps.com/winfx/xaml/controls"
        Title="SpellCastInterrupt" Height="181" Width="253" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" Background="Black">
    <Grid>
        <CheckBox x:Name="p" Content="Interrupt by Movement" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="p_Click"/>
        <CheckBox x:Name="p1" Content="Cast Push Back by Taken Hit" HorizontalAlignment="Left" Margin="10,33,0,0" VerticalAlignment="Top" Click="p_Click"/>
        <CheckBox x:Name="p2" Content="Unknown - 0x4" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" Click="p_Click"/>
        <CheckBox x:Name="p3" Content="Interrupt" HorizontalAlignment="Left" Margin="10,79,0,0" VerticalAlignment="Top" Click="p_Click"/>
        <CheckBox x:Name="p4" Content="Complete Interrupt on Taken Damage" HorizontalAlignment="Left" Margin="10,102,0,0" VerticalAlignment="Top" Click="p_Click"/>
        <CheckBox x:Name="p5" Content="Unknown - 0x20" HorizontalAlignment="Left" Margin="10,125,0,0" VerticalAlignment="Top" Click="p_Click"/>
    </Grid>
</Controls:MetroWindow>
